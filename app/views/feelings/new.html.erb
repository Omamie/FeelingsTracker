<div class="modal-card p-4 my-3">

      <h2 class=" my-3 "> How Do You Feel Right Now? </h2>
      <div>
        <%= simple_form_for @feeling, url: feelings_path do |f| %>
        <%= f.input :number, input_html: { min: '1', max: '22' }%>
        <%= f.submit "Add Feeling", class: "btn-flat" %>
      <% end %>
      </div>

  </div>
